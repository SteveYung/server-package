<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <com.mumayi.paymentcenter.ui.usercenter.view.MyListView
        android:id="@+id/lv_fine_game"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:cacheColorHint="#00000000"
        android:divider="@null"
        android:fadingEdgeLength="25dip"
        android:listSelector="@color/paycenter_white" />

    <TextView
        android:id="@+id/tv_tip"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:visibility="gone" />

    <com.mumayi.paymentcenter.ui.util.view.Loading
        android:id="@+id/loading_wait"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" />

</RelativeLayout>