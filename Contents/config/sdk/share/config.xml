		</SDK>
		<SDK SDKName = "YixinMoments" SDKType = "2" SDKShowName = "易信朋友圈">
			<param name = "SortId" required = "1" desc = "此平台在您分享列表中的位置，整型，数值越大越靠后" showName = "SortId" bWriteIntoManifest = "0" bWriteIntoClient = "0" />
			<param name = "AppId" required = "1" desc = "填写您在易信收藏上注册到的AppId" showName = "AppId" bWriteIntoManifest = "0" bWriteIntoClient = "0" />
			<param name = "ShareByAppClient" required = "1" desc = "是否使用客户端进行分享" showName = "ShareByAppClient" bWriteIntoManifest = "0" bWriteIntoClient = "0" >
				<option value = "true"/>
				<option value = "false"/>
			</param>
		</SDK>
	</SDKLs>
	<paramLs>
		<param name = "AppKey" required = "1" desc = "ShareSDK的appkey" showName = "AppKey" bWriteIntoManifest = "0" bWriteIntoClient = "0" />
	</paramLs>
	
	<operateLs>
		<operate step = "1" name = "modifyManifest" from = "ForManifest.xml" to = "AndroidManifest.xml" desc = "shareSDK"/>
        <operate step = "2" name = "copy" from = "ForRes" to = "res" desc = "shareSDK"/>
        <operate step = "3" name = "mergeR" desc = "shareSDK(合并R文件，此步骤需在res目录合并完后才执行)"/>
		<operate step = "4" name = "script" from = "script.py" desc = "ShareSDK特殊脚本操作"/>
	</operateLs>
	
	<pluginLs>
		<plugin name = "ShareShareSDK" typePlugin = "4" desc = "shareSDK"/>
	</pluginLs>
</config>