		<SDK SDKName = "Renren" SDKType = "2" SDKShowName = "人人">
			<param name = "SortId" required = "1" desc = "此平台在您分享列表中的位置，整型，数值越大越靠后" showName = "SortId" bWriteIntoManifest = "0" bWriteIntoClient = "0" />
