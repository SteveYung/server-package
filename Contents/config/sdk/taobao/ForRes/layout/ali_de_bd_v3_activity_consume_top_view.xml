<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout	
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aligame="http://schemas.android.com/apk/res/com.taobao.de.aligame"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    >
    
    <include layout="@layout/ali_de_bd_v3_activity_consume_info_view" 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />
    <com.taobao.de.aligame.views.BaodianFormatTextContainer
        android:id="@+id/ali_de_bd_v3_id_consume_pay_method_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_gravity="center_horizontal"
        android:gravity="center_horizontal"
        android:layout_marginTop="-10dp"
        android:paddingTop="15dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="20dp"
        android:background="@drawable/ali_de_bd_v3_activity_consume_top_view_paymethod"
        >
        
    </com.taobao.de.aligame.views.BaodianFormatTextContainer>
</LinearLayout>