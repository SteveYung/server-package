<?xml version="1.0" encoding="UTF-8"?>
<com.taobao.de.aligame.views.BaodianCheckboxProtocalView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/id_ali_de_bd_v3_activity_credit_protocal_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="@dimen/ali_de_bd_dimen_credit_margin"
    android:layout_marginRight="@dimen/ali_de_bd_dimen_credit_margin"
    android:layout_marginTop="25dp"
    android:orientation="horizontal"
    >

    <CheckBox
        android:id="@+id/id_ali_de_bd_v3_activity_credit_protocal_checkbox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:button="@drawable/ali_de_bd_v3_consume_checkbox_selector" />

    <TextView
        android:id="@+id/id_ali_de_bd_v3_activity_credit_protocal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_marginLeft="5dp"
        android:clickable="false"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:singleLine="true"
        android:textColor="@color/ali_de_bd_color_black"
        android:textSize="12sp"
        />

</com.taobao.de.aligame.views.BaodianCheckboxProtocalView>
