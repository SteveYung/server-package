<?xml version="1.0" encoding="utf-8"?>
<manifestConfig xmlns:android="http://schemas.android.com/apk/res/android">

    <permissionCfg>
        <uses-permission android:name="android.permission.INTERNET"/>
    </permissionCfg>
    <applicationCfg keyword="com.rsdk.framework.PayActivity">
        <activity
            android:name="com.rsdk.framework.PayActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name="com.rsdk.framework.PayResultActivity"
            android:screenOrientation="landscape" >
            <intent-filter>
               <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:scheme="poqapptest" />
            </intent-filter>
        </activity>  
    </applicationCfg>
</manifestConfig>
