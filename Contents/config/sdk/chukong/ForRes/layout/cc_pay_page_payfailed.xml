<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:cocosdk="http://schemas.android.com/apk/res-auto"
    style="@style/CCPageTheme" >

    <com.coco.sdk.ui.widget.CCPageTitle
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        cocosdk:title_logo="none"
        cocosdk:title_type="only_back" />

    <TextView
        android:id="@+id/errorinfo_text"
        style="@style/ErrorInfo" />

    <TextView
        android:id="@+id/errorinfo_page"
        style="@style/ErrorInfo" />

    <ImageView
        android:id="@+id/cc_pay_fail_img"
        style="@style/PayfailImage" />

    <TextView
        android:id="@+id/cc_pay_fail_text"
        style="@style/PayfailText"
        android:layout_above="@+id/support_num_tv" />

    <Button
        android:id="@+id/close_payfailed"
        style="@style/Payfailbtn"
        android:paddingLeft="5dp"
        android:paddingRight="5dp" />

    <TextView
        android:id="@+id/support_num_tv"
        style="@style/Payfailsupportnum_tv"
        android:layout_above="@+id/close_payfailed" />

</RelativeLayout>