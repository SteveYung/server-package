<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:cocosdk="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    style="@style/CCPageTheme" 
    >

    <com.coco.sdk.ui.widget.CCPageTitle
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        cocosdk:title_type="only_close"
        cocosdk:title_text="@string/cc_user_select_title" />
    <ListView 
        android:id="@+id/list_user"
        android:layout_marginTop="@dimen/cc_height_90"
        android:layout_height="@dimen/cc_height_343"
        android:layout_width="match_parent"
        android:listSelector="@drawable/cc_menu_button"
        android:divider="@drawable/cc_line_menu"
        ></ListView>
</RelativeLayout>
