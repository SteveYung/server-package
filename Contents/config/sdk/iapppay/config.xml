<?xml version="1.0" encoding="UTF-8"?>

<config pluginId = "600024">

  <paramLs>
    <param name="iapppay_appid" required="1" showName="iapppay_appid" desc="AppID" bWriteIntoManifest="0" bUserOffer="1" bWriteIntoClient="1"/>
    <param name="backup" required="1" desc="保留字段不用填" showName="保留" bUserOffer="1" bWriteIntoClient="0"/>
  </paramLs>

  <operateLs>
    <operate step = "1" name = "modifyManifest" from = "ForManifest.xml" to = "AndroidManifest.xml" desc = ""/>
    <operate step = "2" name = "copy" from = "ForLibs" to = "lib" desc = ""/>
    <operate step = "3" name = "copy" from = "ForAssets" to = "assets" desc = ""/>
    <operate step = "4" name = "copy" from = "ForRes" to = "res" desc = ""/>
    <operate step = "5" name = "mergeR" desc = "合并R文件，此步骤需在res目录合并完后才执行" />
  </operateLs>

  <pluginLs>
    <plugin name = "IAPOnlineIapppay" typePlugin = "2" desc = "iap"/>
  </pluginLs>

</config>
