<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sy_corners_background"
    android:orientation="vertical" >

    <View
        android:id="@+id/view1"
        android:layout_width="match_parent"
        android:layout_height="5dp" />

    <WebView
        android:id="@+id/webViewnotice"
        android:layout_width="420dp"
        android:layout_height="31dp"
        android:layout_below="@+id/linearLayout1"
        android:layout_centerHorizontal="true" />

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/webViewnotice"
        android:layout_below="@+id/view1"
        android:background="@drawable/login_port_bg"
        android:orientation="vertical" >

        <View
            android:layout_width="100dp"
            android:layout_height="10dp" />

        <RelativeLayout
            android:id="@+id/sy_title_layer1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal" >

              <ImageView
                        android:id="@+id/sy_title_check"
                        android:layout_width="222dp"
                        android:layout_height="45dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="5dp"
                        android:src="@drawable/regist_com_btn" />
        </RelativeLayout>

        <View
            android:layout_width="100dp"
            android:layout_height="15dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingLeft="31dip"
            android:paddingRight="36dip" >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@drawable/sy_rect_edit"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <LinearLayout
                    android:id="@+id/sy_login_user_img"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/sy_login_user_img1"
                        android:layout_width="33dp"
                        android:layout_height="33dp"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/account_img" />

                    <View
                        android:layout_width="10dp"
                        android:layout_height="match_parent" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:text="账号:"
                        android:textColor="#000000"
                        android:textSize="17sp" />
                </LinearLayout>

                <EditText
                    android:id="@+id/sy_login_edit_username"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@id/sy_login_user_img"
                    android:background="@null"
                    android:ems="10"
                    android:hint="数字和字母组成"
                    android:inputType="textPersonName"
                    android:singleLine="true"
                    android:textColor="#000000"
                    android:textColorHint="#C2C2C2"
                    android:textCursorDrawable="@null"
                    android:textSize="15sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@drawable/sy_rect_edit"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <LinearLayout
                    android:id="@+id/sy_loginkeyimg"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/sy_loginkeyimg1"
                        android:layout_width="33dp"
                        android:layout_height="33dp"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/password_img" />

                    <View
                        android:layout_width="10dp"
                        android:layout_height="match_parent" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:text="密码:"
                        android:textColor="#000000"
                        android:textSize="17sp" />
                </LinearLayout>

                <EditText
                    android:id="@+id/sy_login_edit_password"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@id/sy_loginkeyimg"
                    android:background="@null"
                    android:hint="数字和字母组成"
                    android:inputType="textPersonName"
                    android:singleLine="true"
                    android:textColor="#000000"
                    android:textColorHint="#C2C2C2"
                    android:textCursorDrawable="@null"
                    android:textSize="15sp" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="12dp" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <ImageView
                    android:id="@+id/choose_xieyi"
                    android:layout_width="54dp"
                    android:layout_height="18dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:src="@drawable/choose_yes_img" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/textView1"
                    android:clickable="true"
                    android:onClick="OnClickSeeAgreement"
                    android:text="《93pk用户注册协议》"
                    android:textColor="#447ef7"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/choose_xieyi"
                    android:clickable="true"
                    android:onClick="OnClickSeeAgreement"
                    android:text="我已阅读并同意"
                    android:textColor="#868383"
                    android:textSize="12sp" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="12dp" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <Button
                    android:id="@+id/button1"
                    android:layout_width="122dp"
                    android:layout_height="42dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="17dp"
                    android:background="@drawable/ok"
                    android:onClick="OnClickRegisterBtn"
                    />

                <Button
                    android:layout_width="122dp"
                    android:layout_height="42dp"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginRight="17dp"
                    android:background="@drawable/back"
                    android:onClick="OnClickBackBtn"
                    />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="5dp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/sy_kefu_layer"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="11dp"
        android:background="@drawable/sy_corners_background"
        android:gravity="bottom"
        android:orientation="vertical" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="2dp"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:orientation="horizontal" >

                <ImageView
                        android:id="@+id/sy_kefu_tel_dis"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                       android:layout_gravity="center_vertical"
                        android:src="@drawable/tel" />

                <TextView
                    android:id="@+id/sy_kefu_tel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="020-38551727"
                    android:textColor="#347ef6"
                    android:textSize="13sp" />
            </LinearLayout>

             <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:orientation="horizontal" >
                <ImageView
                        android:id="@+id/sy_kefu_qq_dis"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                       android:layout_gravity="center_vertical"
                        android:src="@drawable/qq" />

                <TextView
                    android:id="@+id/sy_kefu_qq"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="800058592"
                    android:textColor="#347ef6"
                    android:textSize="13sp" />
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>

</RelativeLayout>