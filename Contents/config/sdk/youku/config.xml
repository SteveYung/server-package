        <operate step="6" name="mergeR" desc="合并R文件，此步骤需在res目录合并完后才执行"/>
    </operateLs>
    
    <pluginLs>
        <plugin name="UserYouku" typePlugin="0" desc="youku用户插件"/>
        <plugin name="IAPOnlineYouku" typePlugin="2" desc="youku支付插件"/>
    </pluginLs>
    <version code = "1012" showVersion = "2.1.2_1.6.7" minClientVer="1.5" minFrameworkVer="1.5">
          <![CDATA[更新框架2.0]]>
