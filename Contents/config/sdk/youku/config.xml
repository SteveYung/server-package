<?xml version="1.0" encoding="UTF-8"?>
<config pluginId="73">
    <paramLs>
        <param name="YKGAME_APPID" required="1" showName="YKGAME_APPID" desc="游戏ID" bWriteIntoManifest="1" bUserOffer="1"
               bWriteIntoClient="1"/>
        <param name="YKGAME_APPKEY" required="1" showName="YKGAME_APPKEY" desc="游戏KEY" bWriteIntoManifest="1" bUserOffer="1"
               bWriteIntoClient="1"/>
        <param name="YKGAME_PRIVATEKEY" required="1" showName="YKGAME_PRIVATEKEY" desc="游戏私钥" bWriteIntoManifest="1"
               bUserOffer="1" bWriteIntoClient="1"/>
        <param name="YKGAME_APPNAME" required="1" showName="YKGAME_APPNAME" desc="游戏名" bWriteIntoManifest="1" bUserOffer="1"
               bWriteIntoClient="1"/>
        <param name="callback" required="1" desc="rsdk支付结果通知" showName="支付通知地址" bUserOffer="1" bWriteIntoClient="0"/>
    </paramLs>
    
    <operateLs>
        <operate step="1" name="modifyManifest" from="ForManifest.xml" to="AndroidManifest.xml" desc="youku"/>
