<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"  
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >   	
    <RelativeLayout
        android:id="@+id/lppopwindow_root_RelativeLayout"
        android:layout_width="fill_parent"
    	android:layout_height="fill_parent"    	
    	android:background="#7d000000"
    	android:focusable="false"
        >
    
	<LinearLayout 
	    android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:background="@drawable/lpchange_login_outshape"
    	android:layout_centerInParent="true"
    	android:padding="5dp"
	    >
	    <RelativeLayout 
	         android:id="@+id/lunqi360_layout_pop_root"
	        android:layout_width="500px"
    		android:layout_height="300px"
	        >
	        <ImageButton 
	            android:layout_width="fill_parent"
    			android:layout_height="fill_parent"
    			android:background="@drawable/lpchange_login_inshape"
	            />
	   <LinearLayout 	   
