    		android:layout_height="fill_parent"  
    		android:orientation="vertical"    	
	    	>
	    	<LinearLayout 
        	 	android:layout_width="fill_parent"
    		 	android:layout_height="0px"
    		 	android:layout_weight="50"
    		 	android:gravity="right|center_vertical"
    		 	>
    		 	<ImageButton
    		    	android:id="@+id/lpchange_login_close"
	       			android:layout_width="wrap_content"
	       			android:layout_height="fill_parent"
	       			android:src="@drawable/ebpay_bg_del_selector"
	       			android:scaleType="fitCenter"
	       			android:adjustViewBounds="true"
	       			android:paddingRight="5dp"
	       			android:background="@null"
	       			/>
	    		</LinearLayout >
	    		<!--  -->
	    		 <LinearLayout 
    		    	android:layout_width="fill_parent"
    		 		android:layout_height="0px"
    		 		android:layout_weight="54"
    		 		android:orientation="horizontal" 
    		    	 >
	    		<RelativeLayout 
    		     	android:layout_width="fill_parent"
    		 	 	android:layout_height="fill_parent"
    		 	 	>    		     
    		 
    		 	<LinearLayout 
    		    	android:layout_width="fill_parent"
    		 		android:layout_height="fill_parent"    		 	
    		 		android:orientation="horizontal" >
    		      		<View 
    		    			android:layout_width="0px"
    		 				android:layout_height="fill_parent"
    		 				android:layout_weight="105"
    		     			/>    		         
    		         	<ImageView 
    		             	android:id="@+id/lpchange_login_zhback"
    		             	android:layout_width="0px"
    		 		     	android:layout_height="fill_parent"
    		 		     	android:layout_weight="290"
    		             	android:src="@drawable/an1_lp_mm_bg1"
    		             	android:adjustViewBounds="true"
    		             	android:scaleType="fitCenter"    		             
    		             	/>
    		     		<View 
    		    			android:layout_width="0px"
    		 				android:layout_height="fill_parent"
    		 				android:layout_weight="105"
    		     			/>
    		 	</LinearLayout>
    		  	<LinearLayout 
    		         android:layout_width="fill_parent"
    		         android:layout_height="fill_parent"
    		         android:orientation="horizontal"  
    		         >
    		          <View 
    		    			android:layout_width="0px"
    		 				android:layout_height="fill_parent"
    		 				android:layout_weight="170"
    		     			 />
    		            	  <EditText 
    		            	 	  android:id="@+id/an1_lpchange_loginname"
    		            	      android:layout_width="0px"
    		 					  android:layout_height="fill_parent"
    		 					  android:layout_weight="180"
    		 					  android:gravity="center_vertical"
    		 					  android:background="@null"
    		 					  android:singleLine="true"
    		 					  android:textColor="#ffffff"
    		 					  android:hint="請輸入賬號"
    		 					  android:textColorHint="#ffffff"
    		 					  android:maxLength="20"
    		            	      />
    		              	<ImageButton    
    		              	    android:id="@+id/an1_lpchange_listbut"         
    		             		android:layout_width="0px"
    		 					android:layout_height="fill_parent"
    		 					android:layout_weight="38"
    		             		android:src="@drawable/an1_ddt_main_popdata"
    		             		android:adjustViewBounds="true"
    		             		android:scaleType="fitCenter"
    		             		android:background="@null"    		             		
    		             		/>
    		              	<View 
    		    				android:layout_width="0px"
    		 					android:layout_height="fill_parent"
    		 					android:layout_weight="112"
    		     			 	/>
    		             
    		         </LinearLayout>
    		 	</RelativeLayout>
    		 </LinearLayout>
	    	<!--  -->
	    	<View 
        	 	android:layout_width="fill_parent"
    		 	android:layout_height="0px"
    		 	android:layout_weight="6"/>
	    	<!--  -->
    		 <LinearLayout 
    		    	android:layout_width="fill_parent"
    		 		android:layout_height="0px"
    		 		android:layout_weight="54"
    		 		android:orientation="horizontal" 
    		    	 >
	    		<RelativeLayout 
    		     	android:layout_width="fill_parent"
    		 	 	android:layout_height="fill_parent"
    		 	 	>    		     
    		 
    		 	<LinearLayout 
    		    	android:layout_width="fill_parent"
    		 		android:layout_height="fill_parent"    		 	
    		 		android:orientation="horizontal" >
    		      		<View 
    		    			android:layout_width="0px"
    		 				android:layout_height="fill_parent"
    		 				android:layout_weight="105"
    		     			/>    		         
    		         	<ImageView 
    		             	android:layout_width="0px"
    		 		     	android:layout_height="fill_parent"
    		 		     	android:layout_weight="290"
    		             	android:src="@drawable/an1_lp_mm_bg2"
    		             	android:adjustViewBounds="true"
    		             	android:scaleType="fitXY"    		             
    		             	/>
    		     		<View 
    		    			android:layout_width="0px"
    		 				android:layout_height="fill_parent"
    		 				android:layout_weight="105"
    		     			/>
    		 	</LinearLayout>
    		  	<LinearLayout 
    		         android:layout_width="fill_parent"
    		         android:layout_height="fill_parent"
    		         android:orientation="horizontal"  
    		         >
    		          <View 
    		    			android:layout_width="0px"
    		 				android:layout_height="fill_parent"
    		 				android:layout_weight="170"
    		     			 />
    		            	  <EditText 
    		            	 	  android:id="@+id/an1_lpchange_loginwords"
    		            	      android:layout_width="0px"
    		 					  android:layout_height="fill_parent"
    		 					  android:layout_weight="218"
    		 					  android:gravity="center_vertical"
    		 					  android:background="@null"
    		 					  android:singleLine="true"
    		 					  android:textColor="#ffffff"
    		 					  android:hint="請輸入密碼"
    		 					  android:password="true"
    		 					  android:textColorHint="#ffffff"
    		 					  android:maxLength="20"
    		            	      />    		              	
    		              	<View 
    		    				android:layout_width="0px"
    		 					android:layout_height="fill_parent"
    		 					android:layout_weight="112"
    		     			 	/>
    		             
    		         </LinearLayout>
    		 	</RelativeLayout>
    		 </LinearLayout>
    		 <!--  -->
    		  <View 
        	 	android:layout_width="fill_parent"
    		 	android:layout_height="0px"
    		 	android:layout_weight="6"/>
    		  <!--  -->
    		  <LinearLayout 
        	 android:layout_width="fill_parent"
    		 android:layout_height="0px"
    		 android:layout_weight="38"
    		 android:orientation="horizontal"
    		 android:gravity="center"
    		 >
    		 <View 
    		    android:layout_width="0px"
    		 	android:layout_height="fill_parent"
    		 	android:layout_weight="106"
    		     />
    		<CheckBox    
    			android:id="@+id/an1_ddt_mycheck_sigle"	
    			style="@style/CustomCheckboxTheme"
    			android:gravity="center"
				android:layout_width="0px"
    		 	android:layout_height="fill_parent"
    		 	android:layout_weight="38"
				android:checked="true"/>
    		 <View 
    		    android:layout_width="0px"
    		 	android:layout_height="fill_parent"
    		 	android:layout_weight="10"
    		     />
    		 <ImageView 
    		    android:layout_width="0px"
    		 	android:layout_height="fill_parent"
    		 	android:layout_weight="99"
				android:src="@drawable/an1_ddt_mm_text1"
    		    android:adjustViewBounds="true"
    		    android:scaleType="fitCenter"
    		     />
    		  <View 
    		    android:layout_width="0px"
    		 	android:layout_height="fill_parent"
    		 	android:layout_weight="247"
    		     />
    		
    		 </LinearLayout>
    	<View 
        	 android:layout_width="fill_parent"
    		 android:layout_height="0px"
    		 android:layout_weight="20"/>
	    <LinearLayout 
        	 android:layout_width="fill_parent"
    		 android:layout_height="0px"
    		 android:layout_weight="62"
    		 android:orientation="horizontal"
    		 >
    		   <View 
    		    android:layout_width="0px"
    		 	android:layout_height="fill_parent"
    		 	android:layout_weight="68"
    		     />
    		  <ImageButton 
    		    android:id="@+id/an1_lpchange_regbut"
    		    android:layout_width="0px"
    		 	android:layout_height="fill_parent"
    		 	android:layout_weight="148"				
				android:src="@drawable/an1_ddt_login_butreg"
    		    android:adjustViewBounds="true"
    		    android:scaleType="fitCenter"
    		    android:background="@null"
    		    />
    		   <View 
    		    android:layout_width="0px"
    		 	android:layout_height="fill_parent"
    		 	android:layout_weight="6"
    		     />
    		  <ImageButton
    		    android:id="@+id/an1_lpchange_loginbut"
    		    android:layout_width="0px"
    		 	android:layout_height="fill_parent"
    		 	android:layout_weight="222"		
				android:src="@drawable/lpchangeloginbutnew"
    		    android:adjustViewBounds="true"
    		    android:scaleType="fitCenter"
    		    android:background="@null"
    		    />
    		   <View 
    		    android:layout_width="0px"
    		 	android:layout_height="fill_parent"
    		 	android:layout_weight="6"
    		     />
    		   <ImageButton
    		    android:id="@+id/an1_lpchange_fbbut"
    		    android:layout_width="0px"
    		 	android:layout_height="fill_parent"
    		 	android:layout_weight="148"		
				android:src="@drawable/lpchangefbbut"
    		    android:adjustViewBounds="true"
    		    android:scaleType="fitCenter"
    		    android:background="@null"
    		    />
    		   
    		  <View 
    		    android:layout_width="0px"
    		 	android:layout_height="fill_parent"
    		 	android:layout_weight="68"
    		     />
    		  
	    </LinearLayout >
	    <!--  -->
	    <View 
        	 android:layout_width="fill_parent"
    		 android:layout_height="0px"
    		 android:layout_weight="10"/>
	    	</LinearLayout>
	<!--  -->
   		</RelativeLayout>
  </LinearLayout>
  </RelativeLayout>

</RelativeLayout>
